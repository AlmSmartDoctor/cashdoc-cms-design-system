{
  "name": "@cashdoc/cashdoc-cms-design-system",
  "version": "1.1.0",
  "description": "Cashdoc CMS Design System",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --report-unused-disable-directives",
    "format": "prettier --write .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "screenshot": "storycap http://localhost:6006 -c .storybook/storycap.config.cjs --serverCmd \"pnpm storybook\" --serverTimeout 60000 && find __screenshots__ -type f ! -name \"*For*Jsdoc*\" -delete",
    "screenshot:ci": "storycap http://localhost:6006 -c .storybook/storycap.config.cjs --serverCmd \"pnpm storybook\" --serverTimeout 120000 --verbose && find __screenshots__ -type f ! -name \"*For*Jsdoc*\" -delete"
  },
  "keywords": [
    "design-system",
    "react",
    "components",
    "tailwindcss",
    "storybook"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "peerDependencies": {
    "framer-motion": ">=10.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-dropzone": ">=14.0.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "lucide-react": "^0.555.0",
    "postcss-prefix-selector": "^2.1.1",
    "react-day-picker": "^9.13.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^17.0.0",
    "jiti": "^2.6.1",
    "postcss-rem-to-pixel": "^4.1.2",
    "prettier": "^3.7.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^25.0.2",
    "storybook": "^7.6.17",
    "storycap": "^5.0.1",
    "tailwindcss": "^4.1.6",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.52.0",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.8.3"
  },
  "packageManager": "pnpm@10.27.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
